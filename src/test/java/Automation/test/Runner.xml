<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Login Test Cases">
    <test name="LoginWithValidUsernameValidPassword">
        <classes>
            <class name="Automation.test.LoginPage">
                <methods>
                    <include name="LoginWithValidUsernameValidPassword"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginWithInvalidUsernameInvalidPassword">
        <classes>
            <class name="Automation.test.LoginPage">
                <methods>
                    <include name="LoginWithInvalidUsernameInvalidPassword"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginWithvalidUsernameInvalidPassword">
        <classes>
            <class name="Automation.test.LoginPage">
                <methods>
                    <include name="LoginWithvalidUsernameInvalidPassword"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginWithInvalidUsernamevalidPassword">
        <classes>
            <class name="Automation.test.LoginPage">
                <methods>
                    <include name="LoginWithInvalidUsernamevalidPassword"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>


